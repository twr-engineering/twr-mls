# Test Database Configuration
# Copy this file to test.env and fill in your test database credentials
# IMPORTANT: test.env should NEVER be committed to git

# Test Database (local PostgreSQL)
DATABASE_URL=postgres://USER:PASSWORD@HOST:PORT/DATABASE_NAME
PAYLOAD_SECRET=your_payload_secret_here

# Node Options
NODE_OPTIONS="--no-deprecation --no-experimental-strip-types"

# S3 Configuration (use staging or test bucket)
S3_ENDPOINT=https://your-project.storage.supabase.co/storage/v1/s3
S3_REGION=ap-southeast-1
S3_BUCKET=media
S3_ACCESS_KEY_ID=your_access_key_here
S3_SECRET_ACCESS_KEY=your_secret_key_here
